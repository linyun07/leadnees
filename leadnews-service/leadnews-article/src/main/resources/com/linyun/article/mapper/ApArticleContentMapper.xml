<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.linyun.article.mapper.ApArticleContentMapper">


    <select id="getArticleContentByArticleId" resultType="com.linyun.model.article.pojo.ApArticleContent">
        select *
        from ap_article_content
        where article_id = #{articleId};
    </select>
</mapper>